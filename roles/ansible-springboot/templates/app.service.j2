[Unit]
Description={{ springboot_application_name }}
After=syslog.target

[Service]
User={{ springboot_user }}
ExecStart="/usr/bin/java -jar {{ springboot_deploy_folder }}/{{ springboot_application_name }}.jar"
ExecStop=/usr/bin/pkill -u {{springboot_user}}
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target
